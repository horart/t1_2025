    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "flask/app.py",
                "FLASK_DEBUG": "1",
                "KEEPER_URL": "http://localhost:8001",
                "PROJECTER_URL": "http://localhost:8004",
                "RATER_URL": "http://rater:8002",
                "SEARCHER_URL": "http://searcher:8005",
                "SURVEYER_URL": "http://surveyer:8003"
                        },
            "args": [
                "run",
                "--no-debugger",
                "--port",
                "8000"
            ],
            "jinja": true,
            "autoStartBrowser": false
        },
        {
            "cwd": "${workspaceFolder}/keeper",
            "name": "Python Debugger: keeper",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "env": {
                "DB_PORT": "5435"
            },
            "jinja": true
        },
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "rater.main:app",
                "--reload"
            ],
            "env": {
                "LLM_API_KEY": "sk-EmYVgdtgQpI5dtcukj5otQ"
            },
            "jinja": true
        }
    ]
}